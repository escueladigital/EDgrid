  -ogUrl = "/creditos.html"

  -img = "images/creditos-banner.jpg"
  -titleBanner = "Créditos"  
  -descriptionBanner = "Colaboradores de EDgrid"
  -clase = "accent"
  -boton = ""
  -botonClass= ""
