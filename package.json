{
  "name": "ed-grid",
  "version": "1.3.0",
  "description": "Framework de CSS puro para crear dise√±os web responsive y mobile first.",
  "style": "ed-grid.scss",
  "main": "ed-grid.scss",
  "author": "Alvaro Felipe <informes@escueladigital.pe>",
  "contributors": [
    {
      "name": "Daniel Guillermo Romero Gelvez",
      "email": "danielromeroauk@gmail.com",
      "url": "http://adsiar.com"
    },
    {
      "name": "Carlos Eduardo Cuatin Guaitarilla",
      "email": "cecg1996@gmail.com",
      "url": "http://carloscuatin.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/escueladigital/ED-GRID"
  },
  "bugs": {
    "url": "https://github.com/escueladigital/ED-GRID/issues"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pug": "gulp pug",
    "sass": "gulp sass",
    "es6": "gulp es6",
    "serve": "browser-sync start --server --startPath \"/public/dist\" --files \"./public/dist/**/*.+(html|css|js)\"",
    "build:dev": "npm run pug && npm run sass && npm run es6",
    "watch:pug": "onchange \"./public/src/pug/**\" -- npm run pug",
    "watch:sass": "onchange \"./public/src/scss/**\" -- npm run sass",
    "watch:es6": "onchange \"./public/src/js/**\" -- npm run es6",
    "watch:dev": "parallelshell \"npm run serve\" \"npm run watch:pug\" \"npm run watch:sass\" \"npm run watch:es6\"",
    "dev": "npm run build:dev && npm run watch:dev",
    "img": "gulp image",
    "svg": "gulp svg",
    "webp": "gulp webp",
    "fonts": "gulp fonts",
    "staticsCss": "gulp staticsCss",
    "staticsPhp": "gulp staticsPhp",
    "build:statics": "npm staticsCss && npm run fonts && staticsPhp",
    "build:media": "npm run img && npm run svg && npm run webp",
    "css": "gulp css",
    "js": "gulp js",
    "html": "gulp html",
    "build:prod": "npm run css && npm run js && npm run html",
    "watch:css": "onchange \"./public/dist/css/**\" -- npm run css",
    "watch:js": "onchange \"./public/dist/js/**\" -- npm run js",
    "watch:html": "onchange \"./public/dist/*.html\" -- npm run html",
    "watch:prod": "parallelshell \"npm run serve\" \"npm run watch:css\" \"npm run watch:js\" \"npm run watch:html\"",
    "start": "npm run build:prod && npm run watch:prod",
    "postinstall": "npm start"
  },
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-preset-es2015": "^6.14.0",
    "browser-sync": "^2.16.0",
    "cssnano": "^3.7.5",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^2.0.12",
    "gulp-concat": "^2.6.0",
    "gulp-flatten": "^0.3.1",
    "gulp-htmlmin": "^2.0.0",
    "gulp-image": "^2.7.0",
    "gulp-postcss": "^6.2.0",
    "gulp-pug": "^3.0.4",
    "gulp-pug-lint": "^0.1.6",
    "gulp-sass": "^2.3.2",
    "gulp-sass-lint": "^1.2.0",
    "gulp-svgmin": "^1.2.2",
    "gulp-uglify": "^2.0.0",
    "gulp-useref": "^3.1.2",
    "gulp-webp": "^2.3.0",
    "onchange": "^3.0.0",
    "parallelshell": "^2.0.0"
  },
  "dependencies": {
    "animatescroll.js": "^1.0.7",
    "clipboard": "^1.5.12",
    "jquery": "^2.2.4",
    "picturefill": "^3.0.2",
    "prismjs": "^1.5.1"
  }
}
